{
    "name": "carstingaxion/gatherpress-references",
    "description": "Display production references including clients, festivals and awards in a structured, chronological format.",
    "license": "GPL-2.0+",
    "type": "wordpress-plugin",
    "keywords": [
        "wordpress",
        "wordpress-plugin",
        "gatherpress"
    ],
    "authors": [
        {
            "name": "figuren.theater",
            "email": "info@figuren.theater"
        },
        {
            "name": "Carsten Bach",
            "email": "mail@carsten-bach.de",
            "homepage": "https://carsten-bach.de",
            "role": "Developer"
        }
    ],
    "homepage": "https://websites.fuer.figuren.theater",
    "support": {
        "email": "info@figuren.theater",
        "issues": "https://github.com/carstingaxion/gatherpress-references/issues",
        "source": "https://github.com/carstingaxion/gatherpress-references"
    },
    "require-dev": {
        "figuren-theater/code-quality": "*"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        }
    ],
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true,
            "figuren-theater/altis-core": true,
            "figuren-theater/ft-core": true,
            "koodimonni/composer-dropin-installer": true,
            "phpstan/extension-installer": true
        },
        "sort-packages": true
    },
    "extra": {
        "altis": {
            "install-overrides": []
        }
    }
}